{
  "manifest_version": 2,
  "name": "Wut.link Chrome Integration",
  "description": "Extention to generate a wut.link url from your currently viewed url",
  "version": "0.0.1",

  "permissions": [
    "http://www.wut.link/",
    "https://www.wut.link/",
    "http://wut.link/",
    "https://wut.link/",
    "tabs",
    "storage"
  ],
  "web_accessible_resources":[
    "app/"
  ],
  "browser_action": {
    "default_icon": "wut.png",
    "default_popup": "popup.html"
  },
  "icons": { 
    "128": "wut.png" 
  }
}
